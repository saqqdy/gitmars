{
	"name": "gitmars",
	"description": "这是一个git工作流工具",
	"version": "5.1.3",
	"private": false,
	"bin": {
		"gitm": "lib/gitm.mjs"
	},
	"exports": {
		".": {
			"types": "./typings/index.d.ts",
			"import": "./lib/index.mjs"
		},
		"./lib": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.mjs"
		},
		"./lib/*": {
			"types": "./lib/*.d.ts",
			"import": "./lib/*.mjs"
		},
		"./*.js": {
			"import": "./lib/*.mjs"
		},
		"./*": "./*"
	},
	"imports": {
		"#lib/*": {
			"require": "./lib/*.js",
			"import": "./lib/*.mjs"
		},
		"#conf/*": {
			"require": "./lib/conf/*.js",
			"import": "./lib/conf/*.mjs"
		}
	},
	"files": [
		"lib",
		"typings",
		"*.d.ts"
	],
	"scripts": {
		"build": "rimraf lib es && node --max-old-space-size=8192 node_modules/.bin/rollup -c",
		"dev": "cross-env BUILD_ENV=DEV rollup -c -w",
		"eslint": "eslint --fix --ext .ts,.js,.cjs,.mjs ./",
		"prettier": "prettier --write \"**/*.{js,ts,json,md}\""
	},
	"directories": {
		"bin": "bin",
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"dependencies": {
		"@gitmars/core": "workspace:*",
		"@jssj/request": "^1.1.0",
		"chalk": "^4.1.2",
		"ci-info": "^3.8.0",
		"columnify": "^1.6.0",
		"commander": "^10.0.1",
		"cosmiconfig": "^8.1.3",
		"cross-spawn": "^7.0.3",
		"dayjs": "^1.11.7",
		"debug": "^4.3.4",
		"inquirer": "^9.2.0",
		"js-cool": "^3.1.0",
		"lodash-es": "^4.17.21",
		"lodash-unified": "^1.0.3",
		"node-apollo": "^1.2.1",
		"ora": "^5.4.1",
		"os-locale": "^5.0.0",
		"shelljs": "^0.8.5",
		"slash": "^3.0.0"
	},
	"devDependencies": {
		"@eslint-sets/eslint-config-ts": "^5.1.0",
		"@types/columnify": "^1.5.1",
		"@types/cross-spawn": "^6.0.2",
		"@types/inquirer": "^8.2.6",
		"@types/lodash-es": "^4.17.7",
		"@types/node": "^18.16.1",
		"@types/shelljs": "^0.8.12",
		"cross-env": "^7.0.3",
		"eslint": "^8.39.0",
		"prettier": "^2.8.8",
		"prettier-config-common": "^1.4.0",
		"tsnd": "^1.1.0",
		"typescript": "^5.0.4"
	},
	"sideEffects": false,
	"keywords": [
		"gitmars",
		"git",
		"tool"
	],
	"license": "GPL-3.0",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://www.saqqdy.com/gitmars",
	"bugs": {
		"url": "https://github.com/saqqdy/gitmars/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/gitmars.git",
		"directory": "packages/gitmars"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
