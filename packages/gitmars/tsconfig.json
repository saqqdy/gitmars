{
	"extends": "../../tsconfig.json",
	"exclude": ["**/__tests__"],
	"compilerOptions": {
		"declaration": true,
		"emitDeclarationOnly": true,
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext"],
		"sourceMap": false,
		"rootDir": ".",
		"outDir": "lib",
		"baseUrl": ".",
		"paths": {
			"#lib/*": ["./src/*"],
			"#conf/*": ["./src/conf/*"]
		}
	}
}
