{
	"name": "@gitmars/core",
	"description": "gitmars核心程序",
	"version": "5.3.0",
	"private": false,
	"main": "./lib/index.js",
	"module": "./lib/index.mjs",
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"require": "./lib/index.js",
			"import": "./lib/index.mjs"
		},
		"./*": {
			"types": "./lib/*.d.ts",
			"require": "./lib/*.js",
			"import": "./lib/*.mjs"
		},
		"./lib/*": {
			"types": "./lib/*.d.ts",
			"require": "./lib/*.js",
			"import": "./lib/*.mjs"
		}
	},
	"imports": {
		"#lib/*": {
			"require": "./lib/*.js",
			"import": "./lib/*.mjs"
		},
		"#typings/*": {
			"require": "./typings/*.d.ts",
			"import": "./typings/*.d.ts"
		}
	},
	"files": [
		"cache/.gitkeep",
		"lib",
		"sh",
		"typings",
		"*.d.ts"
	],
	"scripts": {},
	"directories": {
		"bin": "bin",
		"lib": "lib",
		"src": "src",
		"cache": "cache",
		"sh": "sh",
		"typings": "typings"
	},
	"dependencies": {
		"@jssj/request": "^1.1.0",
		"chalk": "^4.1.2",
		"ci-info": "^3.8.0",
		"columnify": "^1.6.0",
		"commander": "^11.0.0",
		"cosmiconfig": "^8.3.5",
		"cross-spawn": "^7.0.3",
		"dayjs": "^1.11.9",
		"debug": "^4.3.4",
		"inquirer": "^9.2.11",
		"js-cool": "^5.7.2",
		"lodash-es": "^4.17.21",
		"lodash-unified": "^1.0.3",
		"node-apollo": "^1.2.1",
		"ora": "^5.4.1",
		"os-lang": "^3.2.0",
		"semver-diff": "^4.0.0",
		"shelljs": "^0.8.5",
		"slash": "^3.0.0"
	},
	"devDependencies": {
		"@eslint-sets/eslint-config-ts": "^5.8.0",
		"@types/cross-spawn": "^6.0.3",
		"@types/inquirer": "^8.2.6",
		"@types/lodash-es": "^4.17.9",
		"@types/node": "^20.6.0",
		"@types/shelljs": "^0.8.12",
		"cross-env": "^7.0.3",
		"eslint": "^8.49.0",
		"madge": "^6.1.0",
		"prettier": "^3.0.3",
		"prettier-config-common": "^1.4.0",
		"tsnd": "^1.1.0",
		"typescript": "^5.2.2"
	},
	"sideEffects": false,
	"keywords": [
		"gitmars",
		"git",
		"tool"
	],
	"license": "GPL-3.0",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/gitmars/tree/master/packages/core#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/gitmars/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/gitmars.git",
		"directory": "packages/core"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
