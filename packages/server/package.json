{
	"name": "@gitmars/server",
	"description": "gitmars可视化界面服务端",
	"version": "5.3.0",
	"private": false,
	"bin": {
		"gitm-server": "./lib/www.mjs"
	},
	"module": "./lib/www.mjs",
	"exports": {
		".": {
			"types": "./lib/www.d.ts",
			"import": "./lib/www.mjs"
		},
		"./*": {
			"types": "./lib/*.d.ts",
			"import": "./lib/*.mjs"
		},
		"./lib/*": {
			"types": "./lib/*.d.ts",
			"import": "./lib/*.mjs"
		}
	},
	"imports": {
		"#lib/*": {
			"import": "./lib/*.mjs"
		},
		"#lib/*.json": "./lib/*.json"
	},
	"files": [
		"lib",
		"typings",
		"*.d.ts"
	],
	"scripts": {
		"server:start": "node lib/www",
		"server:watch": "nodemon --config nodemon.json",
		"server:watch:debug": "nodemon --config nodemon.json --inspect-brk",
		"lib": "sh scripts/lib.sh",
		"build:server": "rimraf app && sh scripts/build-server.sh",
		"dist": "run-s eslint prettier build build:server circular build:ui",
		"eslint": "eslint --fix --ext .ts,.js ./",
		"prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
		"prettier:docs": "prettier --write \"**/*.md\""
	},
	"directories": {
		"app": "app",
		"typings": "typings"
	},
	"dependencies": {
		"@gitmars/core": "workspace:*",
		"@gitmars/ui": "workspace:*",
		"@jssj/request": "^1.1.0",
		"cookie-parser": "^1.4.6",
		"debug": "^4.3.4",
		"express": "^4.18.2",
		"fast-glob": "^3.3.1",
		"gitmars": "workspace:*",
		"hbs": "^4.2.0",
		"http-errors": "^2.0.0",
		"js-cool": "^5.7.2",
		"log4js": "^6.9.1",
		"logger": "^0.0.1",
		"lowdb": "^6.1.0",
		"node-pty": "^1.0.0",
		"os": "^0.1.2",
		"os-lang": "^3.2.0",
		"shelljs": "^0.8.5",
		"socket.io": "^4.7.2",
		"tracer": "^1.1.6",
		"userdir": "^1.0.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@eslint-sets/eslint-config-ts": "^5.8.0",
		"@types/cookie-parser": "^1.4.4",
		"@types/debug": "^4.1.8",
		"@types/express": "^4.17.17",
		"@types/http-errors": "^2.0.1",
		"@types/logger": "^0.0.2",
		"@types/node": "^20.6.0",
		"@types/shelljs": "^0.8.12",
		"@types/uuid": "^9.0.3",
		"eslint": "^8.49.0",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"tsnd": "^1.1.0",
		"typescript": "^5.2.2"
	},
	"sideEffects": false,
	"keywords": [
		"gitmars",
		"server",
		"git",
		"tool"
	],
	"license": "GPL-3.0",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/gitmars/tree/master/packages/server#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/gitmars/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/gitmars.git",
		"directory": "packages/server"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
