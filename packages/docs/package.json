{
    "name": "@gitmars/docs",
    "description": "gitmars文档库",
    "version": "4.0.0-beta.1",
    "private": false,
    "files": [
        "dist",
        "typings",
        "*.d.ts"
    ],
    "scripts": {
        "docs:dev": "vitepress dev .",
        "docs:build": "vitepress build .",
        "docs:serve": "vitepress serve .",
        "eslint": "eslint --fix --ext .ts,.js ./",
        "prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
        "prettier:docs": "prettier --write \"**/*.md\""
    },
    "directories": {
        "dist": "dist",
        "es": "es",
        "lib": "lib",
        "typings": "typings"
    },
    "dependencies": {
        "js-cool": "^2.5.0"
    },
    "devDependencies": {
        "@eslint-sets/eslint-config-vue3-ts": "^3.0.5",
        "@saqqdy/prettier-config": "^1.1.2",
        "@types/markdown-it": "^12.2.3",
        "@types/node": "^18.11.7",
        "eslint": "^8.26.0",
        "markdown-it": "^13.0.1",
        "markdown-it-anchor": "^8.6.5",
        "markdown-it-container": "^3.0.0",
        "prettier": "^2.7.1",
        "tsnd": "^1.1.0",
        "typescript": "^4.8.4",
        "vitepress": "1.0.0-alpha.27"
    },
    "sideEffects": false,
    "keywords": [
        "gitmars",
        "docs",
        "git",
        "tool"
    ],
    "license": "GPL-3.0",
    "author": "saqqdy <https://github.com/saqqdy>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/gitmars.git",
        "directory": "packages/docs"
    },
    "bugs": {
        "url": "https://github.com/saqqdy/gitmars/issues"
    },
    "homepage": "https://github.com/saqqdy/gitmars/tree/master/packages/docs#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
