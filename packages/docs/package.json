{
	"name": "@gitmars/docs",
	"description": "gitmars文档库",
	"version": "5.2.0",
	"private": false,
	"files": [
		"dist",
		"typings",
		"*.d.ts"
	],
	"scripts": {
		"docs:dev": "vitepress dev .",
		"docs:build": "vitepress build .",
		"docs:serve": "vitepress serve .",
		"eslint": "eslint --fix --ext .ts,.js ./",
		"prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
		"prettier:docs": "prettier --write \"**/*.md\""
	},
	"type": "module",
	"directories": {
		"dist": "dist",
		"es": "es",
		"lib": "lib",
		"typings": "typings"
	},
	"dependencies": {
		"js-cool": "^5.7.2"
	},
	"devDependencies": {
		"@eslint-sets/eslint-config-vue3": "^5.8.0",
		"@types/markdown-it": "^13.0.1",
		"@types/node": "^20.6.0",
		"eslint": "^8.49.0",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-container": "^3.0.0",
		"prettier": "^3.0.3",
		"prettier-config-common": "^1.4.0",
		"tsnd": "^1.1.0",
		"typescript": "^5.2.2",
		"vitepress": "1.0.0-rc.10"
	},
	"sideEffects": false,
	"keywords": [
		"gitmars",
		"docs",
		"git",
		"tool"
	],
	"license": "GPL-3.0",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/gitmars.git",
		"directory": "packages/docs"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/gitmars/issues"
	},
	"homepage": "https://github.com/saqqdy/gitmars/tree/master/packages/docs#readme",
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
