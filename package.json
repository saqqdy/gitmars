		"shelljs": "0.8.4",
		"slash": "^3.0.0",
		"text-table": "^0.2.0",
		"tracer": "1.1.4"
	},
	"devDependencies": {
		"@babel/cli": "7.12.10",
		"@babel/core": "7.12.10",
		"@babel/preset-env": "7.12.11",
		"babel-eslint": "^10.1.0",
		"eslint": "7.17.0",
