{
    "name": "gitmars",
    "description": "这是一个git工作流工具",
    "version": "1.21.1",
    "scripts": {
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs:deploy": "GH=1 sh scripts/deploy-docs.sh",
        "server:start": "node app/bin/www",
        "server:watch": "nodemon --config nodemon.json",
        "server:watch:debug": "nodemon --config nodemon.json --inspect-brk",
        "server:super": "supervisor app/bin/www",
        "lib": "sh scripts/lib.sh",
        "build:ui": "sh scripts/build-ui.sh",
        "build:server": "rimraf app && sh scripts/build-server.sh",
        "build": "rimraf lib es && node --max-old-space-size=8192 node_modules/.bin/rollup -c",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p bin/",
        "patch-types": "node scripts/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "circular": "madge lib/ -c",
        "dev": "cross-env BUILD_ENV=DEV rollup -c -w",
        "dist": "run-s eslint prettier build build:server circular build:ui",
        "docs": "rimraf typedoc && typedoc",
        "eslint": "eslint --fix --ext .ts,.js ./",
        "prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
        "prettier:docs": "prettier --write \"**/*.md\""
    },
    "main": "lib/gitm.js",
    "bin": {
        "gitm": "lib/gitm.js"
    },
    "files": [
        "app",
        "cache",
        "lib",
        "typings"
    ],
    "directories": {
        "app": "app",
        "bin": "bin",
        "es": "es",
        "lib": "lib",
        "src": "src",
        "cache": "cache",
        "server": "server",
        "ui": "ui",
        "typings": "typings"
    },
    "dependencies": {
        "@jssj/request": "^1.0.0-alpha.6",
        "ci-info": "^3.3.0",
        "colors": "^1.4.0",
        "columnify": "^1.6.0",
        "commander": "^9.1.0",
        "consola": "^2.15.3",
        "cosmiconfig": "^7.0.1",
        "cross-spawn": "^7.0.3",
        "dayjs": "^1.11.0",
        "inquirer": "^8.2.2",
        "js-cool": "^2.3.2",
        "node-apollo": "^1.2.1",
        "ora": "^5.4.1",
        "qs": "^6.10.3",
        "shelljs": "^0.8.5",
        "slash": "^3.0.0",
        "tracer": "1.1.5"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.19.5",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@saqqdy/prettier-config": "^1.1.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/http-errors": "^1.8.2",
        "@types/inquirer": "^8.2.0",
        "@types/logger": "^0.0.1",
        "@types/node": "^17.0.23",
        "@types/shelljs": "^0.8.11",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vssue/api-github-v3": "^1.4.7",
        "@vssue/vuepress-plugin-vssue": "^1.4.8",
        "@vue/theme": "^1.0.1",
        "@vuepress/plugin-back-to-top": "^1.9.7",
        "@vuepress/plugin-medium-zoom": "^1.9.7",
        "babel-eslint": "^10.1.0",
        "cron-parser": "^4.2.1",
        "cross-env": "^7.0.3",
        "esbuild": "^0.14.28",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-sets": "^1.8.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "less": "^4.1.2",
        "less-loader": "7.3.0",
        "madge": "^5.0.1",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.1",
        "rollup-plugin-esbuild": "^4.8.2",
        "rollup-plugin-replace-shebang": "^1.0.0",
        "rollup-plugin-visualizer": "^5.6.0",
        "ts-node": "^10.7.0",
        "tsnd": "^1.0.2",
        "typedoc": "^0.22.13",
        "typedoc-plugin-markdown": "^3.11.14",
        "typescript": "^4.6.3",
        "uglify-js": "^3.15.3",
        "vuepress": "^1.9.7",
        "vuepress-plugin-container": "^2.1.5",
        "vuepress-plugin-smooth-scroll": "^0.0.10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/gitmars.git"
    },
    "keywords": [
        "gitmars",
        "git",
        "tool"
    ],
    "author": "saqqdy <www.saqqdy.com>",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/saqqdy/gitmars/issues"
    },
    "homepage": "https://www.saqqdy.com/gitmars"
}
